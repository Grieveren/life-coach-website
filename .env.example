# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# EmailJS Configuration
VITE_EMAILJS_SERVICE_ID=your_service_id_here
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# Site Configuration
VITE_SITE_URL=https://your-domain.com
VITE_SITE_NAME="Andrea Gray Life Coaching"

# Analytics (Optional)
VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# Contact Information
VITE_CONTACT_EMAIL=contact@your-domain.com
VITE_CONTACT_PHONE=+1-555-123-4567

# Social Media Links (Optional)
VITE_LINKEDIN_URL=https://linkedin.com/in/your-profile
VITE_FACEBOOK_URL=https://facebook.com/your-page
VITE_INSTAGRAM_URL=https://instagram.com/your-profile

# Build Configuration
NODE_ENV=production