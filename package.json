{
  "name": "life-coach-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "package:zip": "npm run build && cd dist && zip -r site.zip .",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "build:preview": "npm run build && npm run preview",
    "build:production": "NODE_ENV=production npm run build",
    "build:staging": "NODE_ENV=staging npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run format && npm run lint:fix && npm run type-check && npm run test",
    "deploy:netlify": "npm run build:production && netlify deploy --prod --dir=dist",
    "deploy:netlify:preview": "npm run build:staging && netlify deploy --dir=dist",
    "deploy:vercel": "npm run build:production && vercel --prod",
    "deploy:vercel:preview": "npm run build:staging && vercel",
    "deploy:github": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh github-pages",
    "deploy:auto": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh",
    "deploy:build": "node scripts/build-and-deploy.js",
    "deploy:build:staging": "node scripts/build-and-deploy.js netlify staging",
    "deploy:build:preview": "node scripts/build-and-deploy.js netlify production --preview",
    "deploy:test": "node scripts/deploy-simple.js",
    "clean": "rm -rf dist node_modules/.vite",
    "postinstall": "npm run type-check"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "terser": "^5.43.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^3.2.4"
  }
}
